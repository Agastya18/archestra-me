{
  "name": "gmail-oauth-proxy",
  "version": "1.0.0",
  "description": "OAuth proxy service for Gmail MCP Server authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg server.js --output gmail-oauth-proxy && node build.js",
    "dev": "nodemon server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "scripts": [
      "server.js"
    ]
  }
}
